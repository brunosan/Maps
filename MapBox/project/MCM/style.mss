

#mcma {
  marker-width:4;
  marker-fill:#f45;
  marker-allow-overlap:true;
  ['runners'>0] { marker-width:5;}
  ['runners'>10] { marker-width:15;}
  ['runners'>50] { marker-width:20;}
  ['runners'>200] { marker-width:30;}
  ['runners'>500] { marker-width:50;}
  
  ['Time_10'>2] { marker-fill:#79ff00;}
  ['Time_10'>3.3] { marker-fill:#fcd200;}
  ['Time_10'>3.7] { marker-fill:#fc9c02;}
  ['Time_10'>4] { marker-fill:#ff0000;}

}


